package pojo;

public class Test {

	public static void main(String[] args) {
       String str = "ij am santhosh";
       
       
       String split[]=str.split(" ");
       for(int i=split.length-1;i>=4;i--)
       {
    	  
    	   StringBuffer sbu = new StringBuffer(split[i].toString());
    	   System.out.print(sbu.reverse()+ " ");
       }

       
	}

}
