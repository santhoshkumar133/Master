		public static void main(String[] args) {
			public class triangle {

				public static void main(String[] args) {
					
					int row=5;
					
					
				for(int i=5;i>=row;--i)
				{
					for(int j=0;j<=i;++j)
					{
						System.out.print(j+ " ");
					}
					System.out.println(" ");
				}

				}

			}

}
