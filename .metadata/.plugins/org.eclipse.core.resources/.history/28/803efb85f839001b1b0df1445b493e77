package pojo;

public class Test {

	public static void main(String[] args) {
       String str = "ij am santhosh";
       StringBuilder sb= new StringBuilder();
       String split[]=str.split(" ");
       for(int i=split.length-1;i>=0;i--)
       {
    	   System.out.println(split[i]+ " ");
       }
       sb.append(split[]);
	}

}
